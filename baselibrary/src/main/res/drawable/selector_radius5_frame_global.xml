<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_checked="true">
        <shape android:shape="rectangle" android:state_enabled="true">
            <corners android:radius="@dimen/dp5"/>
            <stroke
                android:color="@color/color_global"
                android:width="@dimen/dp1" />
        </shape>
    </item>
    <item android:state_checked="false" android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp5"/>
            <stroke
                android:color="@color/color_EFEFEF"
                android:width="@dimen/dp1" />
        </shape>
    </item>
</selector>